
@{
    ViewBag.Title = "GetAllEmployee";
}

<h2>GetAllEmployee</h2>

@*3 emp information*@

<table class="table-bordered table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in ViewBag.EmpInfo)
    {
        <tr>
            <td>
                @item.EmpId
            </td>
            <td>
                @item.EmpName
            </td>
            <td>
                @item.EmpSalary
            </td>
        </tr>
    }
</table>